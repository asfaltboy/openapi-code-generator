{
  "name": "openapi-code-generator",
  "version": "0.0.1",
  "description": "",
  "main": "./dist/index.js",
  "scripts": {
    "clean": "rm -rf ./dist ",
    "prebuild": "npm run clean",
    "build": "tsc -p ./tsconfig.json",
    "test": "mocha --require source-map-support/register ./dist/**/*.spec.js",
    "analyse": "eslint ./src --cache --fix --ext .js,.jsx,.ts,.tsx",
    "ci-build": "npm run build",
    "ci-test": "nyc mocha --reporter mocha-teamcity-reporter -r ts-node/register './src/**/*.spec.ts'",
    "ci-analyse": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "ci-pipeline": " npm run ci-build && npm run ci-test && npm run ci-analyse"
  },
  "author": "Michael Nahkies",
  "license": "UNLICENSED",
  "devDependencies": {
    "@types/convict": "^6.0.1",
    "@types/js-yaml": "^4.0.0",
    "@types/lodash": "^4.14.168",
    "@types/prettier": "^2.2.3",
    "nodemon": "2.0.7",
    "nyc": "*",
    "ts-node": "9.1.1",
    "typescript": "4.2.4"
  },
  "dependencies": {
    "ajv": "^8.0.5",
    "ajv-formats": "^2.0.2",
    "convict": "^6.0.1",
    "js-yaml": "^4.0.0",
    "lodash": "^4.17.21",
    "prettier": "^2.2.1",
    "source-map-support": "^0.5.19",
    "tslib": "^2.2.0"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "reporter": [
      "lcov",
      "teamcity",
      "text"
    ],
    "all": true
  }
}
